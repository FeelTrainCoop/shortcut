{
  "name": "shortcut-client",
  "description": "Shortcut Client",
  "version": "1.1.0",
  "license": "GPL-3.0",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "docs": "jsdoc -c jsdoc.json",
    "dist": "npm run copy & webpack --env=dist && mkdir -p ../server/public && cp -R ./dist/* ../server/public",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server-auth.js | redis-server | node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FeelTrainCoop/shortcut.git"
  },
  "author": {
    "name": "Darius Kazemi",
    "email": "darius@feeltrain.com"
  },
